  "main": "index.js",
  "scripts": {
    "prestart": "rimfaf dist",
    "watch:server": "rollup -c rollup.server.config.js -w",
    "watch:client": "rollup -c rollup.client.config.js -w",
    "dev:server": "npm-run-all --parallel watch:server sync:server",
