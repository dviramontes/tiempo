    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-latest": "^6.24.1",
    "babelrc-rollup": "^3.0.0",
    "browser-sync": "^2.18.8",
    "eslint": "^3.19.0",
    "nodemon": "^1.11.0",
